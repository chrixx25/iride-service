{
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,
    "target": "esnext",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "strict": true,
    "skipLibCheck": true,
    // "baseUrl": "./",
    // "paths": {
    //   "*": ["src/*"],
    //   "@/*": ["src/*"],
    // },
    // "typeRoots": ["node_modules/@types", "src/global-types/env.d.ts"]
  },
  "include": [
    "./api/*.ts",
    "src/**/*.ts",
    "src/global-types/*.ts",
    "env.d.ts"
  ],
  "exclude": ["node_modules"]
}